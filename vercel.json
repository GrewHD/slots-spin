{
    "version": 2,
    "builds": [
        {
            "src": "src/**/*",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        },
        {
            "src": "storybook-static/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/storybook",
            "dest": "/storybook-static"
        }
    ]
}